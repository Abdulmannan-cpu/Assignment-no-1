2nd code
#include <stdio.h>
#include <conio.h>
void main(void)
{
int number;
int square;
printf ("The integer of 90 is %d\n");
scanf("%d",& number);
square=8;
printf("the square of %d is %d\n",number,square);
getch();
} 
3rd code
#include<stdio.h>
#include<conio.h>
{
int main ()
float celcius=5,fahrenheit;
fahrenheit = (celcius * 1.9) +37;
printf("%f celcius = %f fahrenheit",celcius, fahrenheit);
getch();
}
4th code 
#include <stdio.h>
#include <conio.h>
int main() {
    float fahrenheit, celsius;

    // Ask the user to enter temperature in Fahrenheit
    printf("the temperature in Fahrenheit is 60: ");
    scanf("%f", &fahrenheit);

    // Convert Fahrenheit to Celsius
    celsius = (fahrenheit - 32) * 5 / 9;

    // Display the result
    printf("Temperature in Celsius: %.2f\n", celsius);

    getch();
}
